#include <iostream>
#include <stdio.h>
#include <cmath>
#include <vector>

/*c) Utilice una instrucción de repetición for para inicializar cada elemento de tabla con la
suma de sus subíndices.
Suponga que las variables enteras i y j se definieron como variables de control.*/

#define d 3
int m[3][3];
int i,j;

int main(){
	for (i=0; i<d; i++){
		for (j=0; j<d; j++){
			m[i][j]= i +j;
			printf("%d ", m[i][j]);
		}
	printf("\n");
	}
	return 0;
}
